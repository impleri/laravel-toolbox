{
    "name": "impleri/laravel-toolbox",
    "type": "library",
    "description": "A set of tools for building modular Laravel sites.",
    "license": "BSD-2-Clause",
    "keywords": ["laravel", "laravel 4", "tools"],
    "authors": [
        {
            "name": "Christopher Roussel",
            "email": "christopher@impleri.net"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.1.*",
        "illuminate/console": "4.1.*",
        "illuminate/filesystem": "4.1.*"
    },
    "require-dev": {
        "mockery/mockery": "0.8.*",
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-4": {
            "Impleri\\Toolbox\\": "src/"
        }
    },
    "minimum-stability": "stable"
}
